{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "project": {
        "name": "mindmapp"
    },
    "apps": [
        {
            "root": "src",
            "outDir": "dist",
            "assets": [
                "assets",
                "icon.png"
            ],
            "index": "index.html",
            "main": "main.ts",
            "polyfills": "polyfills.ts",
            "tsconfig": "tsconfig.app.json",
            "prefix": "app",
            "styles": [
                "styles.css"
            ],
            "scripts": [
                "../node_modules/d3/build/d3.min.js",
                "../node_modules/mmp/build/mmp.min.js"
            ]
        }
    ],
    "defaults": {
        "styleExt": "css",
        "component": {}
    }
}
